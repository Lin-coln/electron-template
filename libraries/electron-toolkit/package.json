{
  "name": "@lib/electron-toolkit",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js",
      "require": "./src/*.cjs"
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "@types/node": "^22.7.9",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
